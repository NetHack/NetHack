#	date.c should be recompiled any time any of the source or include code
#	is modified.
$(DATEOBJ): $(DATESRC) $(HACK_H) $(HACKCSRC) $(ALLOBJS)
	$(TARGET_CC) $(TARGET_CFLAGS) $(GITHASH) $(GITBRANCH) \
		/OBJECT=$@ $(DATESRC)

tags: $(CSOURCES)
	@echo ctags -tw ...
	@ctags -tw $(CSOURCES)
	@( cd $(INCL) ; ctags -tw $(HSOURCES) )
	@( cd $(UTIL) ; $(MAKE) tags )

