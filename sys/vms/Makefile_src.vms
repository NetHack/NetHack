# rules
%.obj: $(SRC)%.c
	$(CC) $(CFLAGS) $< /OBJECT=$@

%.obj: $(SYSVMS)%.c
	$(CC) $(CFLAGS) $< /OBJECT=$@

%.obj: $(SYSSHR)%.c
	$(CC) $(CFLAGS) $< /OBJECT=$@

%.obj: $(SYSSHR)%.cpp
	$(CXX) $(CXXFLAGS) $< -o $@

%.obj: $(LUASRCDIR)%.c
	$(CC) $(CFLAGS) /define=(LUA_USE_C89) $< /OBJECT=$@

