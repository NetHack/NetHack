WINCURSESSRC  = $(addsuffix .c, $(addprefix $(WINCURS), $(WINCURSESFILES)))
WINX11SRC     = $(addsuffix .c, $(addprefix $(WINX11), $(WINX11FILES)))
WINQTSRC      = $(addsuffix .cpp, $(addprefix $(WINQT), $(WINQTFILES)))
