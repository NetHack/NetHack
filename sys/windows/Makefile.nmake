	$(OPDC)pdcsetsc.o $(OPDC)pdcutil.o

PDCLIB = $(LIBDIR)\$(PDCDIST)-$(TARGET_CPU).lib

PDCINCL = /I$(PDCURSES_TOP) /I$(PDCSRC) /I$(PDCWINCON)
!ELSE
PDCLIB =
