CURLPDCSRC=https://github.com/Bill-Gray/PDCursesMod/archive/refs/tags/v4.3.5.zip
CURLPDCDST=pdcurses.zip

$(LUASRC)\lua.h:
	cd $(LIBDIR)
	curl -L $(CURLLUASRC) -o $(CURLLUADST)
	tar -xvf $(CURLLUADST)
	cd ..\src
