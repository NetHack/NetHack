	$(link) $(lflags) -o$@ $(ALLOBJ) $(TTYOBJ) $(O)consoletty.o $(O)tile.o \
		$(O)guistub.o $(O)date.o $(O)conres.o $(PDCLIB) $(LUALIB) \
		$(conlibs) -static -lstdc++
	$(subst /,\,@if exist $(O)install.tag del $(O)install.tag)

