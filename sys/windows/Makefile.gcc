	echo /* nhlua.h - generated by top Makefile */ > $@
	@echo #include "$(LUASRC)/lua.h" >> $@
	@echo LUA_API int (lua_error) (lua_State *L) NORETURN; >>$@
	@echo #include "$(LUASRC)/lualib.h" >> $@
	@echo #include "$(LUASRC)/lauxlib.h" >> $@
	@echo /*nhlua.h*/ >> $@

