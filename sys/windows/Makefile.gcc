#==========================================

$(OBJ)/%.o : $(TTY)/%.c
	$(cc) $(CFLAGS)  -o$@  $<

#==========================================
# Rules for files in win/win32
#==========================================

$(OBJ)/%.o : $(MSWIN)/%.c
	$(cc) $(CFLAGS)  -o$@  $<

#==========================================
# Rules for files in win/curses
#==========================================

$(OBJ)/%.o : $(WCURSES)/%.c
	$(cc) -DPDC_NCMOUSE $(PDCINCL) $(CFLAGS) -o$@  $<

#==========================================
# Rules for files in PDCurses
#==========================================

$(OBJ)/%.o : $(PDCURSES_TOP)/%.c
	$(cc) $(PDCINCL) $(CFLAGS)  -o$@  $<

$(OBJ)/%.o : $(PDCSRC)/%.c
	$(cc) $(PDCINCL) $(CFLAGS)  -o$@  $<

