luac.exe: $(O)luac.o $(LUALIB)
	$(link) $(LFLAGSU) -o$@ $(O)luac.o $(LUALIB)

$(LUADLL): $(LUAOBJFILES)
	$(cc) -shared -Wl,--export-all-symbols \
